  "license": "MIT",
  "dependencies": {
    "@hestjs/core": "^0.1.10",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "@hestjs/core": ">=0.1.0",
    "typescript": ">=5.0.0"
  },
  "exports": {
