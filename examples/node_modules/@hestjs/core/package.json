{
  "name": "@hestjs/core",
  "version": "0.1.5",
  "description": "HestJS Core Framework - A TypeScript framework built on Hono with dependency injection and decorators",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build",
    "publish:npm": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aqz236/hest.git"
  },
  "homepage": "https://github.com/aqz236/hest#readme",
  "bugs": {
    "url": "https://github.com/aqz236/hest/issues"
  },
  "author": "aqz236",
  "license": "MIT",
  "dependencies": {
    "@hestjs/logger": "0.1.3",
    "hono": "^4.8.9",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "untyped": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "hestjs",
    "framework",
    "hono",
    "typescript",
    "dependency-injection",
    "decorators",
    "web-framework",
    "backend",
    "api"
  ]
}
